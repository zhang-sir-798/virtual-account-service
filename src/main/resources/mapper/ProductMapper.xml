<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.finance.ema.mapper.ProductMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.com.finance.ema.model.entity.Product">
        <result column="id" property="id" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="product_no" property="productNo" />
        <result column="product_name" property="productName" />
        <result column="single_min_limit" property="singleMinLimit" />
        <result column="single_max_limit" property="singleMaxLimit" />
        <result column="day_max_limit" property="dayMaxLimit" />
        <result column="status" property="status" />
        <result column="create_by" property="createBy" />
        <result column="update_by" property="updateBy" />
        <result column="person_day_max_limit" property="personDayMaxLimit" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        create_time,
        update_time,
        product_no, product_name, single_min_limit, single_max_limit, day_max_limit, status, create_by, update_by,person_day_max_limit
    </sql>

</mapper>
